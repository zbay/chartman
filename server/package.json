{
  "name": "server",
  "version": "0.0.0",
  "description": "description",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "NODE_ENV=dev nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "prestart:test": "rimraf dist && npm run build",
    "start:prod": "node -r ts-node/register -r tsconfig-paths/register dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^5.7.4",
    "@nestjs/core": "^5.7.4",
    "@nestjs/jwt": "^0.3.0",
    "@nestjs/swagger": "^2.5.1",
    "aws-sdk": "^2.418.0",
    "bcrypt": "^3.0.4",
    "class-sanitizer": "0.0.5",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "custom-env": "^1.0.0",
    "express-rate-limit": "^3.4.0",
    "jsonwebtoken": "^8.5.0",
    "jwt-decode": "^2.2.0",
    "morgan": "^1.9.1",
    "nest-router": "^1.0.9",
    "node-cron": "^2.0.3",
    "pg": "^7.8.2",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.2.2",
    "typescript": "^3.0.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.7.4",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^10.12.30",
    "@types/node-cron": "^2.0.1",
    "@types/pg": "^7.4.13",
    "@types/supertest": "^2.0.5",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.8.0",
    "tslint": "5.11.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
